*
    padding 0
    margin 0
    font-family "Roboto", Arial, sans-serif;
    text-decoration none

body, html
    width 100%

a
    color #eee

.header
    background radial-gradient(circle, rgba(9,195,196,1) 50%, rgba(11,100,157,1) 100%)
    height 935px
    max-width 2475px
    margin auto
    display flex
    justify-content center
    align-items center

.header_container
    margin 0 auto
    width 1602px
    max-width 100%
    height 1000px
    position relative

.header__back
    background-image url(../images/Nike_back_background.png)
    background-repeat no-repeat
    background-size contain
    margin 0 auto
    position absolute
    width 100%
    max-width 1552px
    height 452px
    opacity 0.1
    transform translate(1.5%,50%)
    display flex

.header__menu
    position absolute   
    transform translate(-50%, 0%)
    left 50%
    top 60px
    max-width 1370px
    width 100%
    height 100px
    display flex
    align-items center
    flex-wrap wrap

.menu__items
    display flex
    width 1350px
    height 28px
    flex-wrap wrap

.menu__item 
    display flex
    width 50%
    height 30px

.nike-icon
    align-items flex-start
    width 50%

.menu-icon
    justify-content space-around
    flex-direction column
    align-items flex-end

.menu-icon__item
    width 28px
    height 4px
    background-color white

.header__bubble-items
    max-width 1370px
    width 100%
    height 880px
    position absolute
    transform translate(0%,3%)

.header__bubble-item
    border 6px solid #14d0d5
    border-radius 50%
    position absolute
    opacity .4

.header__bubble-item:nth-child(1)
    width 10px
    height 10px
    top 20%
    left 10%

.header__bubble-item:nth-child(2)
    width 102px
    height 102px
    top 5%
    right 15%

.header__bubble-item:nth-child(3)
    border 4px solid #14d0d5
    width 7px
    height 7px
    top 8%
    left 45%
        
.header__bubble-item:nth-child(4)
    border 4px solid #14d0d5
    width 9px
    height 9px
    top 2%
    right 45%

.header__bubble-item:nth-child(5)
    border 4px solid #14d0d5
    width 7px
    height 7px
    bottom 30%
    left 30%

.header__bubble-item:nth-child(6)
    border 2px solid #14d0d5
    width 6px
    height 6px
    top 4%
    left 35%

.header__bubble-item:nth-child(7)
    border 2px solid #14d0d5
    width 4px
    height 4px
    top 6%
    right 68%

.header__bubble-item:nth-child(8)
    width 102px
    height 102px
    bottom 15%
    left 10%

.header__bubble-item:nth-child(9)
    width 10px
    height 10px
    bottom 15%
    right 20%

.header__bubble-item:nth-child(10)
    border 2px solid #14d0d5
    width 4px
    height 4px
    top 49%
    right 50%

.header__board 
    display flex
    justify-content center
    align-items center
    flex-direction column
    width 440px
    height 280px
    position absolute
    transform translate(25%,50%)
    top 180px
    left 100px
    max-width

.header__board_text
    width 440px
    height 145px
    display flex
    align-items center
    justify-content center
    margin 0 auto
    font-size 40px
    color #00fffc

.header__board_buttons
    display flex
    flex-direction row
    margin-top 60px

.header__board_button
    width 200px
    height 48px
    border-radius 50px 50px
    display flex
    align-items center
    justify-content center
    text-transform uppercase
    font-size 14px
    box-shadow 0px 18px 30px 0px rgba(122, 92, 126, 0.12)
    border 2px solid #4bf1f6
    transition 0.8s

.header__board_button:hover 
    -webkit-transform scale(1.2)
    background-color #00fffc

.header__board_button_left
    margin-right 30px

.header__boots
    position absolute
    max-width 820px
    max-height 620px
    width 60%
    height 60%
    top 15%
    right 0%

.header__boots-img
    max-width 100%
    max-height 100%

.banner
    background-image url(../images/banner.png)
    background-repeat no-repeat
    background-size contain
    height 1970px
    width 100%
    max-width 2475px
    position relative
    margin -350px auto
    z-index 2

.banner__container
    width 1602px

.banner__boots-img
    max-width 100%
    max-height 100%

.banner__boots
    background-repeat no-repeat
    background-size contain
    max-width 725px
    max-height 625px
    width 60%
    height 60%
    position absolute
    top 25%
    right 25%

.banner__button-container
    width 205px
    height 53px
    position absolute
    bottom 35%
    left 15%
    display flex
    justify-content center
    align-items center

.banner__bubble
    border-radius 50%
    border 2px solid #e5f8fe
    position absolute
    
.banner__bubble-1
    height 55px
    width 55px
    bottom 37%
    left 16.5%

.banner__bubble-2
    height 31px
    width 31px
    top 25%
    right 30%

.banner__bubble-3
    height 31px
    width 31px
    bottom 33%
    left 28%

.banner__bubble-4
    height 16px
    width 16px
    bottom 40%
    left 28.2%

.banner__button
    width 200px
    height 48px
    border-radius 50px
    display flex
    align-items center
    justify-content center
    font-size 14px
    border 2px solid #4bf1f6
    background-color #00ecf4
    text-transform uppercase
    box-shadow 0px 18px 30px 0px rgba(122, 92, 126, 0.12)
    transition 0.8s

.banner__button:hover 
    -webkit-transform scale(1.2)
    background-color #00fffc

.banner__cotainer-boots
    margin auto
    position relative
    transform translate(50%, 10%)
    top 100px
    right 50px
    height 1000px
    width 1200px

.banner__function
    display flex
    justify-content center
    align-items center
    width 240px
    height 40px
    color #00dfee
    font-size 20px
    position absolute 

.banner__function-1
    top 20%
    right 15%

.banner__function-2
    top 39%
    left -10%

.banner__function-3
    top 43%
    right -1%

.banner__dotted-line
    position absolute
    background-repeat no-repeat 
    background-size contain
    width 20%
    height 20%

.banner__dotted-line-1
    max-width 350px
    max-height 230px
    top 23%
    right 25%
    background-image url(../images/dotted_line_1.png)

.banner__dotted-line-2
    width 400px
    height 150px
    top 40%
    left -3%
    background-image url(../images/dotted_line_2.png)

.banner__dotted-line-3
    width 360px
    height 330px
    top 47%
    right 5%
    background-image url(../images/dotted_line_3.png)

.banner__rectangle
    position absolute
    background-repeat no-repeat 
    background-size contain

.banner__rectangle-1
    width 71px
    height 77px
    background-image url(../images/Rectangle.png)
    top 25%
    left 25%

.banner__rectangle-2
    width 71px
    height 77px
    background-image url(../images/Rectangle-2.png)
    top 23%
    right 45%

.banner__rectangle-3
    width 51px
    height 54px
    background-image url(../images/Rectangle-3.png)
    top 25%
    right 15%

.banner__rectangle-4
    width 36px
    height 33px
    background-image url(../images/Rectangle-4.png)
    bottom 35%
    right 17%

.b-like
    z-index 1
    height 1300px
    width 100%
    background-image url(../images/BG_the_way.png)
    background-size cover
    background-repeat no-repeat
    position relative
    top 150%
    margin -450px auto
    
.b-like__image
    position absolute
    bottom 20%
    left 7%

.b-like__info
    width 600px
    height 360px
    position absolute
    bottom 23%
    right 5%
    width 600px
    height 330px
    display flex
    justify-content center
    align-items center
    flex-direction column

.b-like__info-item
    display flex
    justify-content center
    align-items center
    font-size 20px

.b-like__info_text
    display flex
    justify-content center
    align-items none

.b-like__text
    text-align center
    margin 10%

.b-like__info_button
    width 200px
    height 48px
    border-radius 50px
    display flex
    align-items center
    justify-content center
    font-size 14px
    color #eee
    border 2px solid #4bf1f6
    background-color #00ecf4
    text-transform uppercase
    box-shadow 0px 18px 30px 0px rgba(122, 92, 126, 0.12)

.volumetric-model
    background-image url("../images/BG_3D_model.png")
    background-repeat no-repeat
    background-size cover
    height 1230px
    width 100%
    position absolute
    top 260%
    z-index 2

.volumetric-model__image
    position absolute
    top 30%
    left 20%
    background-repeat no-repeat
    background-size cover

.volumetric-model__info
    position absolute
    bottom 20%
    left 10%
    width 570px
    height 160px
    display flex
    justify-content space-between
    flex-direction column

.volumetric-model__info_header-1   
    text-transform uppercase

.volumetric-model__info_header-2
    text-transform uppercase

.volumetric-model__info_container
    display flex
    flex-direction row
    width 240px

.volumetric-model__info_price
    width 110px
    color #f50057
    font-size 35px
    display flex
    align-items center
    justify-content flex-start

.volumetric-model__info_button
    width 130px
    height 48px
    border-radius 50px
    display flex
    align-items center
    justify-content center
    font-size 14px
    color #eee
    border 2px solid #4bf1f6
    background-color #00ecf4
    text-transform uppercase
    box-shadow 0px 18px 30px 0px rgba(122, 92, 126, 0.12)

.volumetric-model__assessment
    width 290px
    height 90px
    position absolute
    bottom 20%
    right 10%
    display flex
    justify-content space-between
    align-items flex-end
    flex-direction column
    color #00ecf4
    font-size 18px

.volumetric-model__assessment_stars-container
    width 290px
    height 45px
    display flex
    flex-direction row
    justify-content center
    align-items center
    box-shadow 0px 18px 30px 0px rgba(122, 92, 126, 0.08)

.volumetric-model__assessment_star
    width 50px
    height 20px
    display flex
    justify-content space-around
    align-items center

.footer
    width 100%
    height 195px
    background-color #333333
    display flex
    align-items center
    justify-content center
    position absolute
    bottom 10%

.footer__contacts
    display flex
    justify-content space-around
    align-items center


.footer__contacts-container
    display flex
    flex-direction column
    justify-content space-around
    align-items flex-start
